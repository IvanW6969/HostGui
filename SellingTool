-- Scripts:

local function IFGYTR_fake_script() -- B.LocalScript 
	local script = Instance.new('LocalScript', B)

	local B = script.Parent.Frame
	if "Bring" = true then
		local HostsName = "Yahiko_Pain7"
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=workspace.Players[HostsName].HumanoidRootPart.CFrame
end
coroutine.wrap(IFGYTR_fake_script)()
local function TDOWVI_fake_script() -- USERNAMEHERE.LocalScript 
	local script = Instance.new('LocalScript', USERNAMEHERE)

	print("Hello world!")
	
end
coroutine.wrap(TDOWVI_fake_script)()
